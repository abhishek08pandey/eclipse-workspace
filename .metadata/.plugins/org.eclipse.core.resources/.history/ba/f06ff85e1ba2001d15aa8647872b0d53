package com.OneRivet.Programme;

public class Palindrome {

	public static void main(String[] args) {
		
		String s = "abhishek";
		String reverce="";
		
		for (int i = s.length()-1; i >=0; i--) {
			reverce=reverce+s.charAt(i);
		}
		
		if(s==reverce) {
			System.out.println("it is palidrum");
			
		}else {
			System.out.println("not palidrum");
		}

	}

}
