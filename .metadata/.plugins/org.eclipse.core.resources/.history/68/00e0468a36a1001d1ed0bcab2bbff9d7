package com.OneRivet.comparableandcomparator;
import java.util.ArrayList;
import java.util.Collections;
import java.util.Comparator;

class Students{
	
	int id;
	String name;
	int age;
	
	Students(int id, String name, int age){
		this.id=id;
		this.name=name;
		this.age=age;
	}
}

class NameComparator{
	
}

public class Comparator2 {

	public static void main(String[] args) {
		
		ArrayList<Student> list = new ArrayList();
		list.add(new Student(10, "prince", 26));
		list.add(new Student(9, "jay", 36));
		list.add(new Student(11, "husain", 46));
		
		System.out.println("Let's sort by name");
		
		Collections.sort(new NameComparator());
	}
}
