package task;


public class Array {

	public static void main(String[] args) {
		
		int[]x = {6, 2 ,6};
		int []j = new int [x.length];
		int y =0;
		
		
		while (x[y]+x[y+1]=='8')) {
			
		}
		for (int i = 0; i < x.length-1; i++) {
			for (int k = 0; k < x.length-1; k++) {
				if((x[i]+x[i+1])=='8') {
					
					System.out.println(x[i]);
					System.out.println(x[i+1]);
				}
			}
			
		}
	}
}
